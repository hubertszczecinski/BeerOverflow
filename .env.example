# Environment Configuration
# Copy this file to .env and adjust for your environment

# Deployment mode: 'development' or 'production'
DEPLOYMENT_MODE=development

# Domain name (used in production)
DOMAIN_NAME=commerzbank.valchak.com

# Email for Let's Encrypt (used in production)
LETSENCRYPT_EMAIL=example@example.com

# Backend configuration
SECRET_KEY=dev-secret-key-change-in-production

# Frontend ports
# In development: HTTP on 8080, HTTPS on 8443
# In production: HTTP on 80, HTTPS on 443
HTTP_PORT=8080
HTTPS_PORT=8443
